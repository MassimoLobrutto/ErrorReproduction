{
    "cypress-cucumber-preprocessor": {
        "stepDefinitions": "cypress/e2e/step_definitions/**/*.steps.js",
        "filterSpecs": true,
        "json": {
            "enabled": true,
            "output": "jsonlogs/log.json",
            "formatter": "cucumber-json-formatter.exe"
        },
        "html": {
            "enabled": true,
            "output": "reports/AUTOMATION_REPORT.html"
        },
        "messages": {
            "enabled": true,
            "output": "jsonlogs/messages.ndjson"
        }
    },
    "dependencies": {
        "@azure/cosmos": "^3.17.0",
        "@azure/service-bus": "^7.7.0",
        "@azure/storage-blob": "^12.11.0",
        "@badeball/cypress-cucumber-preprocessor": "^12.2.0",
        "@bahmutov/cypress-esbuild-preprocessor": "^2.1.3",
        "@esbuild-plugins/node-modules-polyfill": "^0.1.4",
        "@percy/cypress": "^3.1.2",
        "@testing-library/cypress": "^8.0.3",
        "azure-storage": "^2.10.7",
        "cross-env": "^7.0.3",
        "cucumber-html-reporter": "^5.5.0",
        "cypress": "^10.7.0",
        "cypress-azure-keyvault": "^1.0.0",
        "cypress-cucumber-preprocessor": "^4.3.1",
        "cypress-dark": "^1.8.3",
        "cypress-downloadfile": "^1.2.3",
        "cypress-failed-log": "^2.10.0",
        "cypress-file-upload": "^5.0.8",
        "cypress-iframe": "^1.0.1",
        "cypress-plugin-tab": "^1.0.5",
        "cypress-promise": "^1.1.0",
        "cypress-recurse": "^1.23.0",
        "cypress-wait-until": "^1.7.2",
        "cypress-waitfor": "^1.1.0",
        "cypress-xpath": "^2.0.1",
        "fs-extra": "^10.1.0",
        "jsonschema": "^1.4.1",
        "package.json": "^2.0.1",
        "path": "^0.12.7",
        "puppeteer-core": "^15.5.0",
        "shelljs": "^0.8.5",
        "tedious": "^15.1.0",
        "typescript-string-operations": "^1.5.0"
    }
}
